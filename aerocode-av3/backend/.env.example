# Configuração do Banco de Dados MySQL
# Formato: mysql://USUARIO:SENHA@HOST:PORTA/NOME_DO_BANCO
# Exemplos:
# - XAMPP: mysql://root:@localhost:3306/aerocode
# - MySQL Workbench: mysql://root:SUA_SENHA@localhost:3306/aerocode
# - Docker: mysql://root:root@localhost:3306/aerocode
DATABASE_URL="mysql://root:@localhost:3306/aerocode"

# Chave secreta para autenticação JWT
# Mantenha isso seguro em produção!
JWT_SECRET="your-secret-key-here"

# Porta do servidor backend
PORT=3000

# Ambiente de execução
NODE_ENV="development"
